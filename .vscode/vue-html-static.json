{
	"eslint-disable": {
		"prefix": "eslint-disable",
		"body": "eslint-disable $1",
		"description": "Базовый disable"
	},
	"eslint-disable-line": {
		"prefix": "eslint-disable-line",
		"body": "eslint-disable-line $1",
		"description": "disable для текущей линии в файле"
	},
	"eslint-disable-next-line": {
		"prefix": "eslint-disable-next-line",
		"body": "eslint-disable-next-line $1",
		"description": "disable для следующей линии в файле"
	},
	"notifyUserAboutError": {
		"prefix": "notifyUserAboutError",
		"body": "this.\\$notifyUserAboutError(error)",
		"description": "this.$notifyUserAboutError(error)",
		"scope": "javascript"
	},
	"asyncGet": {
		"prefix": "asyncGet",
		"body": [
			"async ${1:methodName}() {",
			"\ttry {",
			"\t\tawait this.\\$http.get('$2')",
			"\t} catch (error) {",
			"\t\tthis.\\$notifyUserAboutError(error)",
			"\t}",
			"},"
		],
		"description": "Async $http.get with  notifyUserAboutError(error)",
		"scope": "javascript"
	},
	"iticon": {
		"prefix": "iticon",
		"body": "<ItIcon name=\"$1\"/>",
		"description": "Не обязательные параметры: width, height",
		"scope": "vue-html"
	},
	"elrowgutter": {
		"prefix": "elrowgutter",
		"body": [
			"<el-row :gutter=\"\\$gutter\">",
			"\t$0",
			"</el-row>"
		],
		"scope": "vue-html"
	},
	"$breakpoints": {
		"prefix": "$breakpoints",
		"body": "\\$breakpoints.${1|xsOnly,smOnly,smAndUp,smAndDown,mdOnly,mdAndUp,mdAndDown,lgOnly,lgAndUp,lgAndDown,xlOnly|}",
		"scope": "vue-html"
	},
	"$breakpoints.xsOnly": {
		"prefix": "$breakpoints.xsOnly",
		"body": "\\$breakpoints.xsOnly",
		"scope": "vue-html"
	},
